Public Sub Button1_Click()
Dim f1 As File
Dim a As String 
Dim aa As String 
  Shell "netcat " & TextBox1.Text & " " & TextBox2.Text & "  < " & TextBox3.text & " > /tmp/txtBrowser.out"
  
  Sleep 8
  f1 = Open "/tmp/txtBrowser.out" For Input
  aa = ""
  While 1
    Line Input #f1, a
    aa = aa & "\n" & a
    If Eof(f1) Then Break
  Wend
  Close #f1
  Message(aa, "ok")
End

